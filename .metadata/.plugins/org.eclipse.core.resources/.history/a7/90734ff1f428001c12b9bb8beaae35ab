package hierarquia.Loja;

public class TestaLoja {
	public static void main(String [] args) {
		
		Livro livro1 = new Livro(666, "A lenda do chupa-cabra", 25.85, "Jubileu da Silva Xablau");
		Livro livro2 = new Livro(616, "A permissão digital", 65.85, "Titio Mark");
		//System.out.println(livro1.equals(livro2));
		System.out.println(livro1.toString());
		System.out.println("---------------------------------");
		System.out.println(livro2.toString());
		
	}	
}
